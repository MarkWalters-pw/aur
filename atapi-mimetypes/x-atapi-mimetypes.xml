<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-3ds-movable">S
    <comment>Nintendo 3DS Private Key</comment>
    <comment xml:lang="en_GB">Nintendo 3DS Private Key</comment>
    <generic-icon name="application-x-executable"/>
    <magic priority="60">
      <match type="string" offset="0" value="SEED"/>
    </magic>
    <glob pattern="*.sed"/>
  </mime-type>
  <mime-type type="application/x-3ds-romfs">S
    <comment>Nintendo 3DS RomFS</comment>
    <comment xml:lang="en_GB">Nintendo 3DS RomFS</comment>
    <generic-icon name="application-x-executable"/>
    <magic priority="60">
      <match type="string" offset="0" value="IVFC"/>
    </magic>
    <glob pattern="*.romfs"/>
  </mime-type>
