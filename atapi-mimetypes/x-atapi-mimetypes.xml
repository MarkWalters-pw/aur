    <comment>Commodore 64 Disk Image</comment>
    <comment xml:lang="en_GB">Commodore 64 Disk Image</comment>
    <generic-icon name="application-x-raw-disk-image"/>
    <glob pattern="*.d64"/>
    <glob pattern="*.D64"/>
  </mime-type>
